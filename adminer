recreate_table($Q,$C,$q,$If,$cd,$x=array()){if($Q!=""){if(!$q){foreach(fields($Q)as$z=>$p){if($x)$p["auto_increment"]=0;$q[]=process_field($p,$p);$If[$z]=idf_escape($z);}}$lg=false;foreach($q
