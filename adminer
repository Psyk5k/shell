hmac($Ca,$Lb,$z,$_g=false){$Va=64;if(strlen($z)>$Va)$z=pack("H*",$Ca($z));$z=str_pad($z,$Va,"\0");$ee=$z^str_repeat("\x36",$Va);$fe=$z^str_repeat("\x5C",$Va);$I=$Ca($fe.pack("H*",$Ca($ee.$Lb)));if($_g)$I=pack("H*",$I);return$I;}function
